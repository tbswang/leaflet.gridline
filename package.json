{
  "name": "leaflet.gridline",
  "version": "1.0.1",
  "description": "A leaflet plugin grid line",
  "main": "dist/index.js",
  "scripts": {
    "start": "rm -r dist && parcel examples/index.html",
    "build": "rm -r dist && parcel build src/index.js --no-source-maps",
    "build:webpack": "NODE_ENV=production webpack",
    "build:example": "rm -rf public && parcel build examples/index.html --public-url ./ --out-dir public --no-source-maps",
    "release": "./release.sh"
  },
  "keywords": [
    "leaflet",
    "geohash",
    "gridline"
  ],
  "author": "tbswang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tbswang/leaflet.gridline"
  },
  "dependencies": {
    "latlon-geohash": "^2.0.0",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.15",
    "power-assert": "^1.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^7.1.5",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-power-assert": "^2.0.0",
    "babel-preset-stage-3": "^6.24.1",
    "cssnano": "^4.1.10",
    "webpack-cli": "^3.3.11",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.1"
  },
  "babel": {
    "presets": [
      "stage-3",
      "latest",
      "power-assert"
    ]
  }
}
